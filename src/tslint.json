{
  "extends": "../tslint.json",
  "rules": {
    "max-line-length": false,
    "no-inferrable-types": false,
    "no-string-literal": false,
    "one-variable-per-declaration": false,
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
